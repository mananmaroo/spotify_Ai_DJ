# Copy this file to `.env` and fill in your Spotify app credentials.
# Create app: https://developer.spotify.com/dashboard
SPOTIPY_CLIENT_ID=your_spotify_client_id
SPOTIPY_CLIENT_SECRET=your_spotify_client_secret

# Optional runtime defaults for worker/CLI usage:
START_TRACK_QUERY=Blinding Lights
TARGET_YEAR=2018
YEAR_WINDOW=5
TRACK_LIMIT=25
# Optional but recommended for user-auth flows; not required for this project path today.
SPOTIPY_REDIRECT_URI=http://localhost:8888/callback
